<?xml version="1.0" encoding="UTF-8"?>
<ivy-settings>
	<settings defaultResolver="main" />
	<!--Authentication required for publishing (deployment). 'Artifactory Realm' is the realm used by Artifactory so don't change it.-->
	<credentials host="superwaba.dnsalias.com" realm="Artifactory Realm" username="admin" passwd="yourPassword" />
	<resolvers>
		<chain name="main">
			<ibiblio name="public" m2compatible="true" root="http://superwaba.dnsalias.com:8081/artifactory/libs-release"
				pattern="[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]" />
		</chain>
	</resolvers>
</ivy-settings>
